{
    "name": "umijs-next-dashboard-boilerplate",
    "version": "0.0.1",
    "description": "An out-of-box UI solution for enterprise applications",
    "license": "MIT",
    "scripts": {
        "-------------------------graph ---------------------------------": "",
        "schemas": "graphql-codegen --config codegen.yml",
        "schemas:watch": "npm run schemas -- --watch",
        "start": "yarn start:dev",
        "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
        "analyze": "cross-env ANALYZE=1 umi build",
        "build": "umi build",
        "serve": "umi-serve",
        "dev": "npm run start:dev",

        "------------------------- Tools ---------------------------------": "",
        "clean-all": "yarn clean-yarn && yarn clean-node && yarn clean-umi",
        "clean-yarn": "rm -rf yarn.lock",
        "clean-node": "rm -rf node_modules",
        "clean-umi": "rm -rf .mfsu.dev && rm -rf src/.umi",
        "i18n-remove-zhCh": "pro i18n-remove --locale=zh-CN --write",
        "postinstall": "umi g tmp",
        "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
        "lint-staged": "lint-staged",
        "lint-staged:js": "eslint --ext .ts,.tsx ",
        "lint:fix": "eslint --fix --cache --ext .ts,.tsx --format=pretty ./src && npm run lint:style",
        "lint:js": "eslint --cache --ext .ts,.tsx --format=pretty ./src",
        "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
        "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
        "precommit": "lint-staged",
        "prettier": "prettier -c --write \"src/**/*\"",
        "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
        "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
        "pretest": "node ./tests/beforeTest",
        "test": "umi test",
        "test:all": "node ./tests/run-tests.js",
        "test:component": "umi test ./src/components",
        "tsc": "tsc --noEmit",
        "========== only who use Yalc for library development ==========": "",
        "ycom": "yalc add @next-dev/component --no-pure",
        "ycore": "yalc add @next-dev/core --no-pure",
        "yhook": "yalc add @next-dev/hooks --no-pure",
        "ypro": "yalc add @next-dev/provider --no-pure",
        "ycom-update": "yalc add @next-dev/component --no-pure",
        "ycore-update": "yalc add @next-dev/core --no-pure",
        "yhook-update": "yalc add @next-dev/hooks --no-pure",
        "ypro-update": "yalc add @next-dev/provider --no-pure",
        "y-add": "yarn ycom && yarn ycore && yarn yhook && yarn ypro",
        "y-update": "yarn ycom-update && yarn ycore-update && yarn yhook-update && yarn ypro-update"
    },
    "lint-staged": {
        "**/*.less": "stylelint --syntax less",
        "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
        "**/*.{js,jsx,tsx,ts,less,md,json}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "dependencies": {
        "@ant-design/icons": "^4.6.3",
        "@ant-design/pro-descriptions": "^1.9.6",
        "@ant-design/pro-form": "^1.35.0",
        "@ant-design/pro-layout": "^6.24.1",
        "@ant-design/pro-table": "^2.49.0",
        "@apollo/client": "^3.4.8",
        "@next-dev/component": "file:.yalc/@next-dev/component",
        "@next-dev/core": "file:.yalc/@next-dev/core",
        "@next-dev/hooks": "file:.yalc/@next-dev/hooks",
        "@next-dev/provider": "file:.yalc/@next-dev/provider",
        "ahooks": "^2.10.9",
        "antd": "^4.16.13",
        "classnames": "^2.3.1",
        "lodash": "^4.17.21",
        "umi": "^3.5.17"
    },
    "devDependencies": {
        "@ant-design/pro-cli": "^2.1.5",
        "@graphql-codegen/cli": "^1.15.0",
        "@graphql-codegen/import-types-preset": "^2.1.1",
        "@graphql-codegen/introspection": "^2.1.0",
        "@graphql-codegen/typescript": "^2.1.1",
        "@graphql-codegen/typescript-operations": "^2.1.1",
        "@graphql-codegen/typescript-react-apollo": "^3.1.1",
        "@types/history": "^4.7.2",
        "@types/jest": "^26.0.0",
        "@types/lodash": "^4.14.172",
        "@types/react": "^17.0.19",
        "@types/react-dom": "^17.0.9",
        "@umijs/fabric": "^2.6.4",
        "@umijs/plugin-blocks": "^2.2.2",
        "@umijs/plugin-esbuild": "^1.3.1",
        "@umijs/preset-ant-design-pro": "^1.3.3",
        "@umijs/preset-dumi": "^1.1.25",
        "@umijs/preset-react": "^1.8.22",
        "@umijs/route-utils": "^1.0.37",
        "@umijs/yorkie": "^2.0.5",
        "antd-dayjs-webpack-plugin": "^1.0.6",
        "babel-plugin-import": "^1.13.3",
        "carlo": "^0.9.46",
        "cross-env": "^7.0.3",
        "cross-port-killer": "^1.3.0",
        "detect-installer": "^1.0.2",
        "enzyme": "^3.11.0",
        "eslint": "^7.1.0",
        "graphql": "^15.5.1",
        "graphql-tag": "^2.12.5",
        "jsdom": "^17.0.0",
        "jsdom-global": "^3.0.2",
        "lint-staged": "^11.1.2",
        "prettier": "^2.3.2",
        "puppeteer-core": "^10.2.0",
        "react-dev-inspector": "^1.6.0",
        "stylelint": "^13.13.1",
        "typescript": "^4.3.5",
        "umi-serve": "^1.9.10"
    },
    "engines": {
        "node": ">=10.0.0",
        "yarn": ">=1.22.10"
    },
    "gitHooks": {
        "commit-msg": "fabric verify-commit"
    }
}